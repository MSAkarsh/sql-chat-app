# Groq API Key (get it free from https://console.groq.com)
GROQ_API_KEY=your_groq_api_key_here

# Optional: OpenAI API Key (if you want to use GPT instead)
# OPENAI_API_KEY=your_openai_api_key_here

# Optional: LangSmith tracing (get API key from https://smith.langchain.com)
# Set to 'true' to enable tracing and monitoring of all LLM calls
# LANGCHAIN_TRACING_V2=true
# LANGSMITH_API_KEY=your_langsmith_api_key_here
# LANGSMITH_PROJECT=your-project-name
